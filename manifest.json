{
    "manifest_version": 2,
    "name": "NotionBlog Trigger",
    "version": "0.0.1",
    "description": "Trigger netlify or vercel webhook to regenerate the website",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": [
          "https://www.notion.so/*"
        ],
        "js": [
          "src/inject.js"
        ]
      }
    ],
    "browser_action": {
        "default_title": "Set your build hook details",
        "default_popup": "popup.html"
      }
  }